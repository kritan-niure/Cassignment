#include <stdio.h>
int main()
{
	int a[5];
	printf("Enter 5 numbers : \n");
	for(int i=0; i<5; i++){
		scanf("%d", &a[i]);
	}
	for(int i=0; i<5; i++){
		for(int j=i+1; j<5; j++){
			if(a[j]>a[i]){
				int temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	printf("The numbers in descending order are: ");
	for(int i=0; i<5; i++){
		printf("%d\t",a[i]);
	}
	printf("\nSecond largest element: %d\n", a[1]);
	return 0;
}
