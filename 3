#include <stdio.h>
int main()
{
    int a[5];
    printf("Enter 5 numbers:\n"); 
	for (int i=0; i<5; i++)
    {	
        scanf("%d", &a[i]);
    }
    int min=a[0];
	int max=a[0];
	for(int i=1; i<5; i++) 
        {
        	    if (a[i]<min) 
            {
        		  min=a[i];
             }
        		if (a[i]>max)
            {
        		    max=a[i];
            }
    	}
	printf("Minimum value: %d\n", min);
	printf("Maximum value: %d", max);
	return 0;
}
